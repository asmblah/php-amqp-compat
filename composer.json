{
    "name": "asmblah/php-amqp-compat",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/AmqpCompat/bootstrap.php"
        ],
        "psr-4": {
            "": "src/Amqp/",
            "Asmblah\\PhpAmqpCompat\\": "src/AmqpCompat/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asmblah\\PhpAmqpCompat\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "nytris/nytris": "^0.1",
        "php-amqplib/php-amqplib": "v3.5.4",
        "psr/log": "^1|^2|^3"
    },
    "require-dev": {
        "asmblah/php-code-shift": "^0.1.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2",
        "tasque/tasque": "^0.1",
        "tasque/event-loop": "^0.1"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
